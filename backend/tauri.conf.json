{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "infinitebbs",
  "version": "0.1.0",
  "identifier": "com.x.infinitebbs",
  "build": {
    "beforeDevCommand": "cd frontend && bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cd frontend && bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}